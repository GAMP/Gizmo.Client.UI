                            <!--user action btn-->
                            <div class="giz-button-wrapper">
                                <GizProgressButton></GizProgressButton>
                                <GizSubDropdownMenu></GizSubDropdownMenu>
                            </div>
                            <!--user action btn-->
                            <div class="giz-app">
                                <div class="giz-app--title">
                                    programs
                                </div>
                                <div class="giz-app--name">
                                    My computer

                <!--/app info-->
                <div class="giz-app-modal--content">
                    <!--heading-->
                    <div class="giz-app-modal--heading">
                        <div class="giz-header-text">
                            02. action / adventure
                        </div>
                    </div>
                    <!--/heading-->
                    <!--title-->
                    <div class="giz-app-modal--title">
                        @Application?.Name
                    </div>
                    <!--/title-->
                    <div class=" giz-app-rating">
                        <Rating Value="@(Application?.Rate ?? 0)" />
                        <span>@Application?.Ratings Ratings</span>
                    </div>
                    <div class="giz-apps-tags">
                        <span class="giz-selected-tag">Multiplayer</span>
                        <span class="giz-selected-tag">Free to Play</span>
                        <span class="giz-selected-tag">MMO</span>
                        <span class="giz-selected-tag">Custom Tag</span>
                        <button class="giz-add-tags client-button giz-tooltip">
                            +2
                            <div class="giz-tooltip-top">
                                <div>
                                    Action
                                </div>
                                <div>
                                    Adventure
                                </div>
                            </div>
                        </button>
                    </div>
                    <div class="giz-modal--text">
                        Fall Guys is a massively multiplayer party game with up to 60 players online in a free-for-all struggle through round after round of escalating chaos until
                        one victor remains!
                    </div>
                    <!--launch btn-->
                    <div class="d-flex">
                        <Button Size="ButtonSizes.Large">launch</Button>

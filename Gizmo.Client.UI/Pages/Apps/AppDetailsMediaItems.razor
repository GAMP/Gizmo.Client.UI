@namespace Gizmo.Client.UI.Pages
@inherits CustomDOMComponentBase

<ClientTab ControlsPosition="TabControlsPositions.Internal">
    @foreach (var link in _appLinkViewStates.Where(a => a.MediaUrlType != AdvertisementMediaUrlType.None))
    {
        <AppDetailsMediaItem Link="@link" @key="link.AppLinkId" />
    }
</ClientTab>
